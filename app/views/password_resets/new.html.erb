<% provide provide(:title, 'Rename') %>
<div class="page-header text-center">
  <h1>Rename</h1>
</div>

<div class="row">
  <div class="jumbotron col-md-6 col-md-offset-3 background-img">
    <%= form_for(:password_reset, url: password_resets_path) do |f| %>
        <%= render 'shared/flash' %>
        <%= f.label :email, class: 'bright-text' %>
        <%= f.email_field :email, class: 'form-control' %>

        <%= f.submit 'Submit', class: 'btn btn-primary btn-grd' %>
    <% end %>
  </div>
</div>