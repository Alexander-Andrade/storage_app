<div class="row">
  <div class="jumbotron col-md-6 col-md-offset-3">
    <%= form_for @folder, html: { multipart:true } do |f| %>
        <%= render 'shared/error_messages', object: f.object %>
        <%=f.label :name %>
        <%= f.text_field :name, class: 'form-control'%>
        <%= f.submit "Save", class: "btn btn-primary" %>

    <% end %>
  </div>
</div>
